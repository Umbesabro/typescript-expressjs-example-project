{
    "name": "typescript-expressjs-example",
    "version": "1.0.0",
    "description": "Example backend application written in typescript",
    "repository": {
        "type": "git",
        "url": "https://github.com/Umbesabro/typescript-expressjs-example-project.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "npm run build && node dist/index.js",
        "build": "npm run lint && tsc",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix",
        "test": "mocha",
        "test:coverage": "nyc mocha"
    },
    "author": "Adam Drag",
    "license": "MIT",
    "keywords": [
        "typescript",
        "nyc",
        "mocha",
        "expressjs"
    ],
    "dependencies": {
        "express": "4.17.1",
        "custom-env": "2.0.1",
        "fs-extra": "8.1.0",
        "selfsigned": "1.10.7",
        "body-parser": "1.19.0"
    },
    "devDependencies": {
        "@types/node": "12.12.2",
        "@types/chai": "4.2.4",
        "@types/mocha": "5.2.7",
        "@types/sinon": "7.5.0",
        "@types/sinon-chai": "3.2.3",
        "@types/express": "4.17.3",
        "@istanbuljs/nyc-config-typescript": "1.0.1",
        "mocha-junit-reporter": "1.23.3",
        "mocha-multi": "1.1.3",
        "mocha-multi-reporters": "1.1.7",
        "chai": "4.2.0",
        "typescript": "3.0.3",
        "nyc": "15.0.0",
        "mocha": "7.1.1",
        "sinon": "9.0.1",
        "sinon-chai": "3.5.0",
        "source-map-support": "0.5.16",
        "tslint": "5.20.1",
        "ts-node": "8.8.1"
    }
}
